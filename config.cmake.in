@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(redev CONFIG HINTS @redev_DIR@)
find_dependency(Kokkos CONFIG HINTS @Kokkos_DIR@)
find_dependency(MPI)

if(@WDMCPL_ENABLE_OMEGA_H@)
    find_dependency(Omega_h CONFIG HINTS @Omega_h_DIR@)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/wdmcpl_core-targets.cmake")
if(@WDMCPL_ENABLE_C@)
    include("${CMAKE_CURRENT_LIST_DIR}/wdmcpl_capi-targets.cmake")
endif()
if(@WDMCPL_ENABLE_Fortran@)
    include("${CMAKE_CURRENT_LIST_DIR}/wdmcpl_fortranapi-targets.cmake")
endif()

include("${CMAKE_CURRENT_LIST_DIR}/wdmcpl-targets.cmake")
# must be called at the end of the config file
check_required_components(wdmcpl)
